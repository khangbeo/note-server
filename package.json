{
  "name": "simple-server",
  "version": "1.0.0",
  "description": "simple web server",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test node --test",
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=development node --watch --inspect index.js",
    "build:ui": "rm -rf dist && cd ../../part2/notes && npm run build && cp -r dist ../../part3/note-server",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && git push",
    "lint": "eslint ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "mongoose": "^8.18.1",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@stylistic/eslint-plugin-js": "^4.4.1",
    "eslint": "^9.35.0",
    "globals": "^16.4.0",
    "supertest": "^7.1.4"
  }
}
